{
  "name": "MobilizeUT",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "prebuild": "expo prebuild",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "web": "expo start --web",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "postinstall": "node scripts/postinstall.js",
    "setup:cloudflare": ".\\scripts\\setup-pnpm.bat",
    "setup:worker": "cd worker && pnpm install && pnpm run db:create",
    "dev:worker": "cd worker && pnpm run dev",
    "deploy:worker": "cd worker && pnpm run deploy",
    "db:migrate": "cd worker && pnpm run db:migrate",
    "db:shell": "cd worker && pnpm run db:shell",
    "worker:logs": "cd worker && wrangler tail",
    "test:api": "curl http://localhost:8787/",
    "install:all": "pnpm install && cd worker && pnpm install"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.1.8",
    "@hookform/resolvers": "^5.2.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.1.18",
    "@turf/turf": "^7.2.0",
    "@types/geojson": "^7946.0.16",
    "clsx": "^2.1.1",
    "expo": "~53.0.23",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-network": "~7.1.5",
    "expo-router": "~5.1.7",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "nativewind": "latest",
    "phosphor-react-native": "^3.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.61.1",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-maps": "^1.20.1",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.20.0",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.3.1",
    "type-fest": "^4.41.0",
    "zod": "^4.0.13",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@types/react": "~19.0.14",
    "eslint": "^9.32.0",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "wrangler": "^3.0.0"
  },
  "private": true,
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js-pure",
      "unrs-resolver"
    ]
  },
  "overrides": {
    "react-native-worklets-core": "0.0.0-invalid"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}